{
  "expo": {
    "name": "Happy",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#2AB5D1"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.sallada.happy"
    },
    "android": {
      "package": "org.sallada.happy"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
